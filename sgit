    printf "%s\n" "Options:"

    # :command.usage_flags
    # :flag.usage
    printf "  %s\n" "--scope, -s SCOPE"
    printf "    Provides a scope consisting of a noun to provide additional contextual\n    information\n"
    echo

    # :flag.usage
