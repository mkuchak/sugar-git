    echo

  fi

  printf "%s\n" "Usage:"
  printf "  sgit sub [OPTIONS] [STRING...]\n"
  printf "  sgit sub --help | -h\n"
  echo

  # :command.long_usage
  if [[ -n $long_usage ]]; then
    printf "%s\n" "Options:"

    # :command.usage_flags
    # :flag.usage
    printf "  %s\n" "--all, -a"
    printf "    Remove all files or directories from staging area\n"
    echo

    # :command.usage_fixed_flags
    printf "  %s\n" "--help, -h"
    printf "    Show this help\n"
    echo

  fi
}

