# :command.parse_requirements
sgit_log_parse_requirements() {
  # :command.fixed_flags_filter
  while [[ $# -gt 0 ]]; do
    case "${1:-}" in
      --help | -h)
        long_usage=yes
        sgit_log_usage
        exit
        ;;

      *)
        break
        ;;

    esac
  done

  # :command.command_filter
  action="log"

  # :command.parse_requirements_while
  while [[ $# -gt 0 ]]; do
    key="$1"
    case "$key" in
      # :flag.case
      --author | -a)

        # :flag.case_arg
        if [[ -n ${2+x} ]]; then

          args['--author']="$2"
          shift
          shift
        else
          printf "%s\n" "--author requires an argument: --author, -a AUTHOR" >&2
          exit 1
        fi
        ;;

      # :flag.case
      --since | -s)

        # :flag.case_arg
        if [[ -n ${2+x} ]]; then

          args['--since']="$2"
          shift
          shift
        else
          printf "%s\n" "--since requires an argument: --since, -s SINCE" >&2
          exit 1
        fi
        ;;

      # :flag.case
      --until | -u)

        # :flag.case_arg
        if [[ -n ${2+x} ]]; then

          args['--until']="$2"
          shift
          shift
        else
          printf "%s\n" "--until requires an argument: --until, -u UNTIL" >&2
          exit 1
        fi
        ;;

      # :flag.case
      --exclude | -e)

        # :flag.case_no_arg
        args['--exclude']=1
        shift
        ;;

      -?*)
        printf "invalid option: %s\n" "$key" >&2
        exit 1
        ;;

      *)
        # :command.parse_requirements_case
        # :command.parse_requirements_case_simple
        if [[ -z ${args['search_terms']+x} ]]; then

          args['search_terms']=$1
          shift
        else
          printf "invalid argument: %s\n" "$key" >&2
          exit 1
        fi

        ;;

    esac
  done

}

